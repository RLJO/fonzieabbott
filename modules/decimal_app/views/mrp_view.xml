<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="focus_areas_tree" model="ir.ui.view">
        <field name="name">focus_area.list.view</field>
        <field name="model">focus_areas</field>
        <field name="arch" type="xml">
            <tree>
               <xpath expr="//form[1]/sheet[1]/notebook[1]/page[@name='components']/field[@name='move_raw_ids']/tree[1]/field[@name='product_uom']" position="before">
                    <field name="qty_on_hand_in_location"/>
              </xpath>
            </tree>
        </field>
    </record>

    

    
</odoo>